# gchat 配置文件
# 复制此文件为 .env 并填入实际值
# cp .env.example .env

# ==================== 必需配置 ====================

# Google Gemini 官方 API Key (可选，用于 --backend official)
GEMINI_API_KEY=

# ==================== R2 存储配置 ====================
# 用于上传生成的图片

R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET=your_bucket_name
R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com
R2_PUBLIC_URL=https://pub-xxx.r2.dev

# ==================== Turing 后端 (Gemini 特价) ====================
# 用途: turing CLI 专用，使用 ZYAI 中转的 Gemini API
# 特点: 比官方 API 更便宜，支持 gemini-3-flash-preview 等模型
TURING_BASE_URL=https://api.zyai.online/v1/chat/completions
# 特价分组的 Gemini Key (turing 专用)
TURING_SPECIAL_GEMINI_GROUP_KEY=sk-your_turing_key

# ==================== 后端 API 配置 ====================

# Google API 代理 (默认后端)
GOOGLE_API_URL=https://your-google-api-proxy.com/v1/chat/completions

# NexusAI 后端
NEXUSAI_URL=https://your-nexusai.com/v1/chat/completions
NEXUSAI_KEY=your_nexusai_key

# 本地后端 (可选)
LOCAL_API_URL=http://localhost:8100/v1/chat/completions

# ==================== 图片生成 API 配置 ====================

# NexusAI 图片生成
NEXUSAI_IMAGE_URL=https://your-nexusai.com/v1/images/generations
NEXUSAI_IMAGE_KEY=sk-your_nexusai_image_key

# GPT-Load 图片生成 (Cookie模式)
GPTLOAD_IMAGE_URL=https://your-gptload.com/proxy/gemini-cookie/v1/images/generations
GPTLOAD_IMAGE_KEY=your_gptload_key

# ZYAI 图片生成
ZYAI_IMAGE_URL=https://api.zyai.online/v1beta/models/{model}:generateContent
ZYAI_IMAGE_KEY=sk-your_zyai_key

# YUNWU 图片生成
YUNWU_IMAGE_URL=https://yunwu.zeabur.app/v1/chat/completions
YUNWU_IMAGE_KEY=sk-your_yunwu_key

# Google API 图片生成 (Cookie模式，无需Key)
GOOGLE_API_IMAGE_URL=https://your-google-api-proxy.com/v1/chat/completions
